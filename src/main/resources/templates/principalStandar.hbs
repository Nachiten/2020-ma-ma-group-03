<body style="background: #757575;
    background-position: center center;
    background-repeat: no-repeat;
    background-attachment: fixed;
    background-size: cover;">
    <div class="wrapper">
        <div class="fijo">
            <div class="multi_color_border"></div>
                <div class="top_nav">
                    <div class="left">
                        <div class="logo"><p style="font-size: 30px;"><span>GESOC</span></p>
                            Bienvenido {{apellido}}, {{nombre}}
                        </div>
                    </div>
                    <div class="right">
                        <ul>
                            <li>
                                <!-- BOTON HOME -->
                                <a href="/inicio" >
                                    <button name="button">
                                        <i class="fas fa-home"></i>
                                        <br>Inicio
                                    </button>
                                </a>
                            </li>

                            <li>
                                <!-- BOTON PERFIL -->
                                <a href="/editarPerfil" >
                                    <button name="button">
                                        <i class="fas fa-user-alt"></i>
                                        <br>Perfil
                                    </button>
                                </a>
                            </li>


                            <li>
                                <!-- BOTON MENSAJES -->
                                <a href="/mensajes">
                                    <button name="button">
                                        <i class="fa fa-envelope" aria-hidden="true"></i>
                                        <br>Mensajes
                                    </button>
                                </a>
                            </li>
                            <li>
                                <!-- CERRAR SESION -->
                                <button name="button"  onclick="cerrarSesion()">
                                    <i class="fas fa-sign-out-alt"></i>
                                    <br>Cerrar sesión
                                </button>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="bottom_nav">
                    <ul class="link">
                        <li class="a"><a href="/ingresos">Ingresos</a></li>
                        <li class="b"><a href="/egresos">Egresos</a></li>
                        <li class="c"><a href="/presupuestos">Presupuestos</a></li>
                        <li class="e"><a href="/listadoOperaciones">Listado de operaciones</a></li>
                        <li class="f"><a href="/asociarOperacion">Asociar operaciones</a></li>
                        <li class="f"><a href="/validadorDeTransparencia">Validador de transparencia</a></li>
                    </ul>
                </div>

        </div>

            <div class="container">
                {{#block "contenido"}}
                {{/block}}
            </div>
    </div>
    <div id="modal" class="modal fade" role="dialog"></div>
</body>
</html>
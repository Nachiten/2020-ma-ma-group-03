{{#partial "contenido"}}
    <div class = "altaProveedor">
        <br>
        <br>
        <form action="">
            <center>
                <table class= "formulario" border = 0>
                    <tr>
                        <td>Nombre:</td>
                        <td><input type="text" name="nombre" placeholder="Ingrese el nombre"  id=" altaProveedor-nombre" required></td>
                    </tr>
                    <tr>
                        <td>Apellido:</td>
                        <td><input type="text" name="apellido" placeholder="Ingrese el apellido" id="altaProveedor-apellido" required></td>
                    </tr>
                    <tr>
                        <td>Razon Social:</td>
                        <td><input type="text" name="dni" placeholder="Ingrese la razon social"  id = "altaProveedor-razonSocial" required></td>
                    </tr>
                    <tr>
                        <td>DNI:</td>
                        <td><input type="number" min="0" name="dni" placeholder="Ingrese el dni" id="altaProveedor-dni" required></td>
                    </tr>
                    <tr>
                        <td>Pais:</td>
                        <td>
                            <select name = "pais" id="altaProveedor-pais" required >
                            <option>Seleccionar pais</option>
                        </td>
                    </tr>
                    <tr>
                        <td>Provincia:</td>
                        <td>
                            <select name = "provincia" id="provincia"required >
                                <option>Seleccionar provincia</option>
                        </td>
                    </tr>
                    <tr>
                        <td>Ciudad:</td>
                        <td>
                            <select name = "ciudad"  id="ciudad" required >
                                <option>Seleccionar provincia</option>
                        </td>
                    </tr>
                    <tr>
                        <td>Calle:</td>
                        <td><input type="text" name="calle" placeholder="Ingrese la calle"  id = "calle" required></td>
                    </tr>
                    <tr>
                        <td>Altura:</td>
                        <td><input type="number" min="0" name="altura" placeholder="Ingrese la altura"  id = "altura" required></td>
                    </tr>
                    <tr>
                        <td>Piso:</td>
                        <td><input type="number"  min="0" name="piso" placeholder="Ingrese la altura" id="piso" required></td>
                    </tr>
                </table>
                <button type= "submit" >Guardar</button>
            </center>
        </form>
    </div>
    <link rel="stylesheet" href="/css/estiloAltaProveedor.css" />
{{/partial}}
{{>principalAdministrador.hbs}}